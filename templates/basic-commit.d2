# Basic Commit Template
# Use this as a starting point for representing a single JJ commit

...@../styles/default-theme.d2

# Single commit node
commit_id: {
  label: "change_id\nCommit message"
  class: commit
}

# For immutable commits, use:
# commit_id: {
#   label: "change_id\nCommit message"
#   class: commit-immutable
# }

# For conflicted commits, use:
# commit_id: {
#   label: "change_id\nCommit message"
#   class: commit-conflict
# }
